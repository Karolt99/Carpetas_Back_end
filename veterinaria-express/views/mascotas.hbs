<table border="solid">
<h2>LISTADO DE MASCOTAS</h2>
<thead>
<tr>
<th>Nombre de la mascota</th>
<th>Nombre del dueño</th>
<th>Cédula del dueño</th>
<th>Edad de la mascota</th>
<th>Teléfono del dueño</th>
<th>Opciones</th>
</tr>
</thead>
<tbody>
{{#each mascotas}}
<tr>
<td>{{nombre}}</td>
<td>{{nombre_duenio}}</td>
<td>{{cedula_duenio}}</td>
<td>{{edad}}</td>
<td>{{telefono_duenio}}</td>
<td><a class="eliminar" href="mascotas/eliminar/{{cedula_duenio}}">Eliminar</a></td>
</tr>
{{/each}}
</tbody>
</table>
<a href="mascotas/agregar">Agregar mascotas</a>